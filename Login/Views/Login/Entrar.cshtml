<h1>Logar</h1>
@if(TempData["erro"] != null)
{
    <div class="alert alert-danger">
        @TempData["erro"]
    </div>
}

<form action="/Login/Entrar" method="post">
    <input name="login" type="text" />
    <br />
    <input name="senha" type="password"/>
    <br />
    <button type="submit">Entrar</button>
</form>